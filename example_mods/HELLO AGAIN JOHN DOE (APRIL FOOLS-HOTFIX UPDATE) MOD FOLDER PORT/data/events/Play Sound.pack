function postCreate()
	for (e in events)
		if (e.name == 'Play Sound') {
			var sound = FlxG.sound.load(Paths.sound(e.params[0]));
            sound.volume = e.params[1];
            myNewSounds.push(sound);
		}

function onEvent(e) if (e.event.name == 'Play Sound') myNewSounds.pop().play(e.event.params[2]);
